# Deployment Private Key (DO NOT COMMIT ACTUAL KEY)
PRIVATE_KEY=your_deployer_private_key_here

# API Keys for RPC Providers
ALCHEMY_API_KEY=your_alchemy_api_key_here
INFURA_API_KEY=your_infura_api_key_here

# Network RPC URLs (Optional - defaults to Alchemy)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
MUMBAI_RPC_URL=https://polygon-mumbai.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
BASE_RPC_URL=https://mainnet.base.org

# Block Explorer API Keys for Contract Verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# Gas Reporting
REPORT_GAS=false
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Initial Token Supply (in KSYS tokens, will be multiplied by 10^18)
INITIAL_SUPPLY=1000000

# Treasury Configuration
TREASURY_SIGNERS=0xAddress1,0xAddress2,0xAddress3
TREASURY_THRESHOLD=2
TREASURY_TIMELOCK=86400

# Owner Address (defaults to deployer if not set)
OWNER_ADDRESS=
